{
	"buildCommand": "cd apps/web && pnpm build",
	"installCommand": "pnpm install --no-frozen-lockfile",
	"framework": "nextjs",
	"outputDirectory": "apps/web/.next",
	"builds": [
		{
			"src": "apps/web/package.json",
			"use": "@vercel/next"
		}
	],
	"env": {
		"NEXT_PUBLIC_SERVER_URL": "https://appadosal-server.vercel.app"
	}
}
